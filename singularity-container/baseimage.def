Bootstrap: library
From: ubuntu:20.04

%post
    chmod 777 /tmp
    cat /etc/apt/sources.list | sed -e 's/main$/universe/' > /etc/apt/sources.list.d/universe.list
    apt-get -y update
    apt-get -y install python3 python3-pip graphviz normaliz nauty libgsl-dev daemon git autoconf
    apt-get -y clean
